@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000

title Initial state

package States {
    class FlashcardDeckState1 as "__fd1:ObservableList<Flashcard>__"

    class Flashcard1State1 as "__f1:Flashcard__"
    class Flashcard2State1 as "__f2:Flashcard__"
    class Flashcard3State1 as "__f3:Flashcard__"
}

Flashcard1State1 --o FlashcardDeckState1
Flashcard2State1  --o FlashcardDeckState1
Flashcard3State1  --o FlashcardDeckState1

class Pointer as "Current State" #FFFFF
Pointer -up-> FlashcardDeckState1
@end
